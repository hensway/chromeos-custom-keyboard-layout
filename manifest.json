{
    "manifest_version": 3,
    "name": "custom keyboard layout",
    "version": "0.0.1",
    "permissions": [
        "input"
    ],
    "input_components": [
        {
            "name": "Layout Name",
            "type": "ime",
            "id": "layout_name",
            "description": "A keyboard layout",
            "language": "en-US",
            "layouts": ["us"]
        }
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}